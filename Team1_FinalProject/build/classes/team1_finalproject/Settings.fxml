<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane id="BorderPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <bottom>
    <HBox alignment="TOP_CENTER" prefHeight="114.0" prefWidth="600.0">
      <children>
        <Button fx:id="btnSaveSettings" focusTraversable="false" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" prefWidth="130.0" text="Save" textFill="WHITE">
          <font>
            <Font size="15.0" fx:id="x1" />
          </font>
          <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="25.0" top="10.0" />
          </HBox.margin>
        </Button>
        <Button fx:id="btnCancelSettings" focusTraversable="false" font="$x1" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" prefWidth="130.0" text="Cancel" textFill="WHITE">
          <HBox.margin>
            <Insets bottom="10.0" left="25.0" right="10.0" top="10.0" />
          </HBox.margin>
        </Button>
      </children>
    </HBox>
  </bottom>
  <center>
    <VBox prefHeight="200.0" prefWidth="100.0">
      <children>
        <HBox alignment="TOP_CENTER">
          <children>
            <Label fx:id="lblUserStatus" text="User Status:">
              <font>
                <Font name="SansSerif Regular" size="16.0" />
              </font>
              <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="30.0" top="25.0" />
              </HBox.margin>
            </Label>
            <TextField fx:id="tfUserStatus" disable="false" editable="false" focusTraversable="false" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="30.0" prefWidth="200.0" text="Admin">
              <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" fx:id="x4" />
              </HBox.margin>
            </TextField>
          </children>
          <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" fx:id="x2" />
          </VBox.margin>
        </HBox>
        <HBox alignment="TOP_CENTER">
          <children>
            <Label fx:id="lblUserPrivileges" text="User Privileges:" HBox.margin="$x2">
              <font>
                <Font size="16.0" fx:id="x3" />
              </font>
            </Label>
            <ChoiceBox id="choicebUser" fx:id="choicebUserPrivileges" focusTraversable="false" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="30.0" prefWidth="200.0" HBox.margin="$x2">
              <items>
                <FXCollections fx:factory="observableArrayList">
                  <String fx:value="Item 1" />
                  <String fx:value="Item 2" />
                  <String fx:value="Item 3" />
                </FXCollections>
              </items>
            </ChoiceBox>
          </children>
        </HBox>
        <HBox alignment="TOP_CENTER">
          <children>
            <Label fx:id="lblNotifyByEmail" font="$x3" text="Notify by Email:">
              <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" fx:id="x4" />
              </HBox.margin>
            </Label>
            <ChoiceBox fx:id="choicebNotifyEmail" focusTraversable="false" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="30.0" prefWidth="200.0" HBox.margin="$x4">
              <items>
                <FXCollections fx:factory="observableArrayList">
                  <String fx:value="Item 1" />
                  <String fx:value="Item 2" />
                  <String fx:value="Item 3" />
                </FXCollections>
              </items>
            </ChoiceBox>
          </children>
        </HBox>
      </children>
    </VBox>
  </center>
  <top>
    <VBox alignment="BOTTOM_CENTER">
      <children>
        <Label id="lblUserSettings" fx:id="lblSettings" text="Settings" textFill="GREY">
          <font>
            <Font name="SansSerif Regular" size="25.0" />
          </font>
          <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="30.0" />
          </VBox.margin>
        </Label>
        <Separator maxWidth="-Infinity" prefWidth="525.0" style=".separator*.line {&#10;-fx-border-style: solid;&#10;-fx-background-color: #D3D3D3;&#10;}" />
      </children>
    </VBox>
  </top>
</BorderPane>
