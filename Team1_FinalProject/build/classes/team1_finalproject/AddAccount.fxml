<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane id="BorderPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <bottom>
    <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
      <children>
        <Button fx:id="btnPasswordGenerator" focusTraversable="false" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" prefWidth="160.0" text="Password Generator" textFill="WHITE">
          <font>
            <Font size="13.0" fx:id="x1" />
          </font>
          <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" fx:id="x2" />
          </HBox.margin>
        </Button>
        <Button fx:id="btnAddAccount" focusTraversable="false" font="$x1" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" prefWidth="160.0" text="Add Account" textFill="WHITE" HBox.margin="$x2" />
        <Button focusTraversable="false" font="$x1" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" prefWidth="160.0" text="Cancel" textFill="WHITE" HBox.margin="$x2" />
      </children>
    </HBox>
  </bottom>
  <center>
    <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
      <children>
        <VBox alignment="TOP_RIGHT" prefHeight="226.0" prefWidth="256.0">
          <children>
            <TextField fx:id="tfAccountName" focusTraversable="false" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="40.0" prefWidth="250.0" promptText="Account Name">
              <VBox.margin>
                <Insets bottom="5.0" left="10.0" right="10.0" top="20.0" fx:id="x4" />
              </VBox.margin>
            </TextField>
            <TextField fx:id="tfAccountUserID" focusTraversable="false" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="40.0" prefWidth="250.0" promptText="User ID">
              <VBox.margin>
                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" fx:id="x3" />
              </VBox.margin>
            </TextField>
            <PasswordField fx:id="tfAccountPassword" focusTraversable="false" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="40.0" prefWidth="250.0" promptText="Password" VBox.margin="$x3" />
            <TextField fx:id="tfAccountURL" focusTraversable="false" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="40.0" prefWidth="250.0" promptText="URL">
              <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
              </VBox.margin>
            </TextField>
          </children>
        </VBox>
        <VBox alignment="TOP_LEFT" prefHeight="226.0" prefWidth="256.0">
          <children>
            <ChoiceBox fx:id="choicebAccountType" focusTraversable="false" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="40.0" prefWidth="250.0" VBox.margin="$x4">
              <items>
                <FXCollections fx:factory="observableArrayList">
                  <String fx:value="Item 1" />
                  <String fx:value="Item 2" />
                  <String fx:value="Item 3" />
                </FXCollections>
              </items>
            </ChoiceBox>
            <TextArea fx:id="taAccountNotes" focusTraversable="false" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="140.0" prefWidth="250.0" promptText="Account Notes" wrapText="true" VBox.margin="$x3" />
          </children>
        </VBox>
      </children>
    </HBox>
  </center>
  <top>
    <VBox alignment="BOTTOM_CENTER">
      <children>
        <Label fx:id="lblAddAccount" text="Add Account" textFill="GREY">
          <font>
            <Font name="SansSerif Regular" size="25.0" />
          </font>
          <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="30.0" />
          </VBox.margin>
        </Label>
        <Separator maxWidth="-Infinity" prefWidth="525.0" style=".separator*.line {&#10;-fx-border-style: solid;&#10;-fx-background-color: #D3D3D3;&#10;}" />
      </children>
    </VBox>
  </top>
</BorderPane>
