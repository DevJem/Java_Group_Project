<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<BorderPane id="BorderPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="team1_finalproject.MainWindowController">
  <bottom>
    <HBox prefHeight="14.0" prefWidth="600.0" />
  </bottom>
  <center>
    <HBox prefHeight="305.0" prefWidth="600.0">
      <children>
        <TreeView id="tvSideMenu" fx:id="treeView" editable="true" focusTraversable="false" prefHeight="312.0" prefWidth="168.0">
          <HBox.margin>
            <Insets />
          </HBox.margin>
        </TreeView>
        <TableView fx:id="tableView" focusTraversable="false" prefHeight="306.0" prefWidth="405.0" HBox.hgrow="ALWAYS">
          <columns>
            <TableColumn prefWidth="75.0" text="Account" fx:id="columnAccount" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="81.0" text="User ID" fx:id="columnUserID" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="82.0" sortable="false" text="Password" fx:id="columnPassword" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="82.0" text="Created" fx:id="columnURL" />
            <TableColumn prefWidth="75.0" text="Modified" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="57.0" sortable="false" text="Notes" fx:id="columnNotes" />
          </columns>
        </TableView>
      </children>
    </HBox>
  </center>
  <top>
    <VBox prefHeight="74.0" prefWidth="600.0">
      <children>
        <MenuBar>
          <menus>
            <Menu mnemonicParsing="false" text="File">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#Settings" text="Save" />
                <MenuItem mnemonicParsing="false" text="Save As..." />
                <MenuItem mnemonicParsing="false" onAction="#Settings" text="Settings" />
                <MenuItem mnemonicParsing="false" onAction="#ExitProgram" text="Exit" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
              <items>
                <MenuItem mnemonicParsing="false" text="Cut" />
                <MenuItem mnemonicParsing="false" text="Copy" />
                <MenuItem mnemonicParsing="false" text="Paste" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Tools">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#passwordGenerator" text="Password Generator" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
              <items>
                <MenuItem mnemonicParsing="false" text="About" fx:id="miAbout" />
              </items>
            </Menu>
          </menus>
        </MenuBar>
        <ToolBar minHeight="50.0" prefHeight="50.0" prefWidth="600.0">
          <items>
            <Button fx:id="btnOpen" contentDisplay="TOP" focusTraversable="false" mnemonicParsing="false" text="Open">
              <graphic>
                <ImageView fitHeight="21.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                    <Image url="@../png/003-folder.png" backgroundLoading="false" />
                  </image>
                </ImageView>
              </graphic>
            </Button>
            <Button fx:id="btnSave" contentDisplay="TOP" focusTraversable="false" mnemonicParsing="false" text="Save">
              <graphic>
                <ImageView fitHeight="21.0" fitWidth="21.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="true">
                  <image>
                    <Image url="@../png/008-floppy-disk.png" />
                  </image>
                </ImageView>
              </graphic>
            </Button>
            <Separator orientation="VERTICAL" prefHeight="36.0" />
            <Button fx:id="btnAdd" contentDisplay="TOP" focusTraversable="false" mnemonicParsing="false" onAction="#addButton" text="Add">
              <graphic>
                <ImageView fitHeight="21.0" fitWidth="21.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="true">
                  <image>
                    <Image url="@../png/002-add.png" />
                  </image>
                </ImageView>
              </graphic>
            </Button>
            <Button fx:id="btnEdit" contentDisplay="TOP" focusTraversable="false" mnemonicParsing="false" onAction="#editButton" text="Edit">
              <graphic>
                <ImageView fitHeight="21.0" fitWidth="21.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="true">
                  <image>
                    <Image url="@../png/009-pencil.png" />
                  </image>
                </ImageView>
              </graphic>
            </Button>
            <Button fx:id="btnDelete" contentDisplay="TOP" focusTraversable="false" mnemonicParsing="false" onAction="#deleteButton" text="Delete">
              <graphic>
                <ImageView fitHeight="21.000099999997474" fitWidth="21.000099999997474" pickOnBounds="true" preserveRatio="true">
                  <image>
                    <Image url="@../png/004-cross.png" />
                  </image>
                </ImageView>
              </graphic>
            </Button>
            <Separator orientation="VERTICAL" prefHeight="36.0" />
            <Button fx:id="btnPasswordGenerator" contentDisplay="TOP" focusTraversable="false" mnemonicParsing="false" onAction="#passwordGenerator" text="Password Generator">
              <graphic>
                <ImageView fitHeight="21.0" fitWidth="21.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="true">
                  <image>
                    <Image url="@../png/010-key.png" />
                  </image>
                </ImageView>
              </graphic>
            </Button>
          </items>
        </ToolBar>
      </children>
    </VBox>
  </top>
</BorderPane>
